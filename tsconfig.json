{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
 /*    "jsx": "react-jsx",
    "jsxImportSource": "react", */
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "/pkg/*": ["public/pkg/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["node", "astro/client"]
  },
  "include": [
    ".astro/types.d.ts",
    "src/**/*", "public/pkg",
  ],
  "exclude": [
    "dist",
    "node_modules",
    "public"
  ]
}